{
  "name": "management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "vite --port 5174",
    "start:srv": "nodemon ./backend/app.js",
    "build": "tsc -b && vite build",
    "postinstall": "node backend/api/setupdb.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently \"npm run start:srv\" \"npm run start:dev\"",
    "db:generate": "cd backend && npx prisma generate",
    "db:push": "cd backend && npx prisma db push",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:studio": "cd backend && npx prisma studio",
    "db:seed": "cd backend && node prisma/dbSeed.js"
  },
  "dependencies": {
